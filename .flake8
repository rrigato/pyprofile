[flake8]
docstring-convention=numpy
exclude=*__init__.py
#E302 = 2 blank lines after functions
#W391 = blank line at end of file
#D414 = docstring Raises section has no content
#D100 = Missing docstring in public module
#D400 = First line should end with a period
ignore=W391, D100, D400
max-line-length=100